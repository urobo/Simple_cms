<% @page_title = "Admin User List"%>
<div class="admin users list">
 <h2> AdminUser </h2>
 <%= link_to("Add New Admin User",{:action => 'new'}, :class => 'action new') %>
 <div> <%= pluralize(@admins.size, 'admin')%></div>
 <table class="listing" summary="Admin list">
 	<tr class="header">
 		
 		
 		<th>Username</th>
 		<th>First Name</th>
 		<th>Last Name</th>
 		<th>Email</th>
 		<th>Actions</th>
 	</tr>
 	<% @admins.each do |admin|%>
 		<tr class=" <%= cycle('odd','even')%> ">
 			<td><%= admin.username %></td>
 			<td><%= admin.first_name %></td>
 			<td><%= admin.last_name %></td>
 			<td><%= admin.email %></td>				

 			<td class="actions">
 				<%= link_to("Show", {:action => 'show', :id => admin.id})%>
 				<%= link_to("Edit", {:action => 'edit',:id => admin.id}, :class => 'action edit') %>
 				<%= link_to("Delete", {:action => 'delete', :id => admin.id}, :class => 'action delete') %>
 			</td>
 		</tr>
 		<% end %>
 </table>
</div>
